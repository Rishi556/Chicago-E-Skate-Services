{
  "compilerOptions": {
    "allowJs": true,
    "target": "es2019",
    "isolatedModules": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "outDir": "./built/",
    "moduleResolution": "node"
  },
  "include": ["*.ts", "utils/*.ts"],
  "exclude": ["node_modules"],
  "assets": ["package.json"]
}